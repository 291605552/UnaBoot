<?xml version="1.0" encoding="UTF-8" ?>
<ehcache name="unaboot-cache">
    <diskStore path="java.io.tmpdir"/>
    <defaultCache
        maxEntriesLocalHeap="2000"
        eternal="false"
        timeToIdleSeconds="3600"
        timeToLiveSeconds="3600"
        overflowToDisk="false">
    </defaultCache>
    <cache name="dayHits"
           maxEntriesLocalHeap="500"
           eternal="true"
           overflowToDisk="true">
    </cache>
</ehcache>